{
    "type": "shell",
    "label": "C/C++: gcc build active file",
    "command": "/usr/bin/gcc",
    "args": [
        "-Wall",
        "-Wextra",
        "-Wpedantic",
        "-Wshadow",
        "-Wformat=2",
        "-Wcast-align",
        "-Wconversion",
        "-Wsign-conversion",
        "-Wnull-dereference",
        "-g3",
        "-O0",
        "-I/root/projects/echo/**",
        "-c",
        "client.c",
        "-o",
        "./build/Debug/client.o",
        "&&",
        "/usr/bin/gcc",
        "-c",
        "client_tls.c",
        "-o",
        "./build/Debug/client_tls.o",
        "&&",
        "/usr/bin/gcc",
        "-c",
        "error_handler.c",
        "-o",
        "./build/Debug/error_handler.o",
        "&&",
        "/usr/bin/gcc",
        "-c",
        "client_socket.c",
        "-o",
        "./build/Debug/client_socket.o",
        "&&",
        "/usr/bin/gcc",
        "./build/Debug/client.o",
        "./build/Debug/client_tls.o",
        "./build/Debug/error_handler.o",
        "./build/Debug/client_socket.o",
        "-o",
        "./build/Debug/outDebug",
        "-lssl",
        "-lcrypto"
    ],
    "options": {
        "cwd": "${fileDirname}"
    },
    "problemMatcher": [
        "$gcc"
    ],
    "group": {
        "kind": "build",
        "isDefault": true
    },
    "detail": "Task generated by Debugger.",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                ""
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}